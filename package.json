{
  "name": "alsa-birthday",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:prod": "vite build --mode production",
    "build:dev": "vite build --mode development",
    "preview": "vite preview",
    "preview:docs": "vite preview --outDir docs",
    "clean": "node -e \"require('fs').rmSync('docs', { recursive: true, force: true })\"",
    "clean:build": "npm run clean && npm run build",
    "start": "vite preview --outDir docs",
    "predeploy": "npm run build:prod",
    "deploy": "gh-pages -d docs"
  },
  "dependencies": {
    "framer-motion": "^10.16.16",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.20.0"
  },
  "devDependencies": {
    "@types/react": "^18.3.1",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.2.1",
    "gh-pages": "^6.3.0",
    "vite": "^5.0.8"
  }
}
